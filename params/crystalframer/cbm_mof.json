{
    "_comment": "CrystalFramer config for CBM-MOF (CH4/N2 separation). Based on default_jarvis.json with adjustments for MOF multi-target regression.",

    "seed": 42,
    "experiment_name": "cbm_mof_cf",
    "save_path": "result/cbm_mof",

    "optimizer": "adamw",
    "lr": 0.0005,
    "lr_sch": "inverse_sqrt_nowarmup",
    "sch_params": [4000.0],
    "adam_betas": [0.9, 0.98],
    "weight_decay": 1e-5,
    "clip_norm": 1.0,
    "clip_grad": 0.0,
    "dropout": 0.0,

    "batch_size": 64,
    "n_epochs": 100,

    "targets": [
        "AdsCH4_10kPa",
        "AdsCH4_100kPa",
        "AdsCH4_1000kPa",
        "AdsN2_10kPa",
        "AdsN2_100kPa",
        "AdsN2_1000kPa",
        "QstCH4",
        "QstN2"
    ],

    "loss_func": "L1",
    "normalize_targets": "scale_bias",

    "encoder_name": "latticeformer",
    "num_layers": 4,
    "lattice_range": 2,
    "adaptive_cutoff_sigma": -3.5,
    "minimum_range": true,
    "model_dim": 128,
    "k_dim": 0,
    "v_dim": 0,
    "head_num": 8,
    "ff_dim": 512,
    "t_fixup_init": true,
    "embedding_dim": [128],
    "t_activation": "relu",

    "domain": "real",
    "gauss_lb_real": 0.5,
    "gauss_lb_reci": 0.5,
    "gauss_state": "q",
    "normalize_gauss": true,
    "positive_func_beta": 0.1,
    "use_cgcnn_feat": false,

    "scale_real": [1.4],
    "scale_reci": [2.2],
    "value_pe_dist_real": 64,
    "value_pe_dist_reci": 0,
    "value_pe_dist_coef": 1.0,
    "value_pe_wave_real": 0,
    "value_pe_wave_reci": 0,
    "value_pe_dist_max": -10.0,
    "value_pe_dist_wscale": 1.0,

    "frame_method": "max",
    "cos_abs": 0,
    "value_pe_angle_real": 64,
    "value_pe_angle_wscale": 4.0,
    "value_pe_angle_coef": 1.0,

    "pooling": "avr",
    "pre_pooling_op": "no",
    "norm_type": "no",
    "freeze_bn_epochs": 0,

    "train_filter_max": 0,
    "train_filter_min": 0,
    "train_percent_check": 1.0,
    "val_percent_check": 1.0,

    "model_checkpoint_save_top_k": 3,
    "pretrained_model": null,
    "num_workers": 4,
    "target_set": "cbm_mof",

    "swa_epochs": 10,
    "scale_grad": 0.0,
    "use_cuda_code": true,
    "reproduciblity_state": 2,
    "symm_break_noise": 1e-5
}
